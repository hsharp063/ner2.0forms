            <uBlogsyPost id="1285" parentID="1288" level="6" creatorID="0" sortOrder="1" createDate="2014-11-02T13:11:40" updateDate="2014-11-02T13:15:05" nodeName="Important Announcement - Corrine" urlName="important-announcement-corrine" path="-1,1112,1157,1162,1287,1288,1285" isDoc="" nodeType="1067" creatorName="admin" writerName="admin" writerID="0" template="1053" nodeTypeAlias="uBlogsyPost">
